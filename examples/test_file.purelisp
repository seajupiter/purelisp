; Sample test file for the Lisp interpreter
; Demonstrates multiple expressions and recursion

; Define some basic values
(def x 10)
(def y 5)

; Define factorial function 
(defun factorial (n)
  (if (<= n 1)
      1
      (* n (factorial (- n 1)))))

; Define fibonacci function
(defun fibonacci (n)
  (if (<= n 1)
      n
      (+ (fibonacci (- n 1)) 
         (fibonacci (- n 2)))))

; Some arithmetic
(+ x y)
(* x y)

; Calculate factorial of 5
(factorial 5)

; Calculate fibonacci of 7
(fibonacci 7)

; A more complex example with let
(let ((a 5)
      (b 3))
  (+ a (* b 2)))