; square root function using Newton's method

(defun newton_it_sqrt (y x n)
    (if (= n 0)
        x
        (let (
            (z (/ (+ x (/ y x)) 2.0)))
            (newton_it_sqrt y z (- n 1)))))

(defun mysqrt (x)
    (if (= x 0.0)
        0.0
        (newton_it_sqrt x 1.0 1000)))

(mysqrt 100.0)
(let ((x (mysqrt 100.0))) (* x x))
(mysqrt 2.0)
(mysqrt 3.0)
